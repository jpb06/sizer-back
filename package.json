{
  "name": "sizer-back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"}",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:db": "docker-compose down && docker-compose up sandbox-postgres",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:dev": "jest --watchAll --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:merge": "ts-node ./scripts/merge.prisma.schemas.ts && yarn prisma format --schema prisma/schema.prisma",
    "prisma:gen": "prisma:merge && prisma generate",
    "prisma:seed": "yarn prisma migrate dev && yarn prisma db seed --preview-feature"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/swagger": "^5.0.4",
    "@nestjs/terminus": "^7.2.0",
    "@prisma/client": "^2.28.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.23.0",
    "google-auth-library": "^7.3.0",
    "jsonwebtoken": "^8.5.1",
    "node-rsa": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^14.14.36",
    "@types/node-rsa": "^1.1.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.18",
    "prettier": "^2.2.1",
    "prisma": "^2.27.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
