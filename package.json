    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.7",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
