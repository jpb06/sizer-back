    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.2",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.4.1",
    "prisma": "^3.1.1",
